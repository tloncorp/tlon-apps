{
  "name": "tlon-mobile",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "android": "expo run:android",
    "bundler": "expo start",
    "clearcache": "expo start -c",
    "ios": "expo run:ios",
    "generate": "npm run generate:tailwind && npm run generate:ios",
    "generate:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "generate:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "format": "npm run format:js && npm run format:swift",
    "format:js": "eslint --fix --quiet ./src && prettier --write .",
    "format:swift": "swiftformat **/*.swift --exclude ./ios/Pods, ./node_modules",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "test": "vitest --watch false",
    "tsc": "tsc --noEmit",
    "postinstall": "npm run generate",
    "eas-build-post-install": "./post-install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,html}": [
      "npm run format:js"
    ],
    "*.swift": [
      "npm run format:swift"
    ]
  },
  "dependencies": {
    "@google-cloud/recaptcha-enterprise-react-native": "^18.3.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@op-engineering/op-sqlite": "^3.0.7",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/app": "^18.4.0",
    "@react-native-firebase/crashlytics": "^18.4.0",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@tloncorp/shared": "*",
    "@tloncorp/ui": "*",
    "@urbit/aura": "^1.0.0",
    "@urbit/http-api": "^3.1.0-dev-2",
    "classnames": "^2.3.2",
    "dotenv-expand": "^11.0.6",
    "expo": "~50.0.14",
    "expo-application": "~5.8.3",
    "expo-asset": "~9.0.2",
    "expo-clipboard": "~5.0.1",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-device": "~5.9.3",
    "expo-file-system": "~16.0.8",
    "expo-localization": "~14.8.3",
    "expo-modules-core": "~1.11.12",
    "expo-notifications": "~0.27.6",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "~11.7.2",
    "expo-updates": "~0.24.12",
    "posthog-react-native": "^2.7.1",
    "react": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-native": "0.73.6",
    "react-native-branch": "^5.9.0",
    "react-native-country-codes-picker": "^2.3.3",
    "react-native-device-info": "^10.8.0",
    "react-native-fetch-api": "^3.0.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-phone-input": "^1.3.7",
    "react-native-polyfill-globals": "^3.1.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-sse": "^1.2.1",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^14.1.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "13.6.4",
    "tailwind-rn": "^4.2.0",
    "text-encoding": "^0.7.0",
    "web-streams-polyfill": "^3.3.3"
  },
  "devDependencies": {
    "@react-native/metro-config": "^0.73.5",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "^18.2.55",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-expo": "^10.0.0",
    "concurrently": "^8.0.1",
    "connect": "^3.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ncp": "^2.0.0",
    "open": "^10.1.0",
    "postcss": "^8.4.26",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react-native-svg-transformer": "^1.3.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.3",
    "vitest": "^1.0.4"
  }
}
