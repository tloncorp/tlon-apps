{
  "name": "@tloncorp/ui",
  "types": "./src",
  "main": "src/index.ts",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "pnpm exec tamagui-build --skip-types && cp -R src/assets/icons/*.svg dist/cjs/assets/icons/ && cp -R src/assets/icons/*.svg dist/jsx/assets/icons/",
    "watch": "mkdir -p dist/cjs/assets/icons && mkdir -p dist/jsx/assets/icons && cp -R src/assets/icons/*.svg dist/cjs/assets/icons/ && cp -R src/assets/icons/*.svg dist/jsx/assets/icons/ && tamagui-build --watch --skip-types",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix --quiet",
    "lint:format": "prettier src/ --write",
    "test": "vitest",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix",
      "pnpm lint:format"
    ]
  },
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@likashefqet/react-native-image-zoom": "^3.0.0",
    "@react-native-clipboard/clipboard": "^1.14.0",
    "@tamagui/animations-moti": "1.93.2",
    "@tamagui/get-token": "1.93.2",
    "@tamagui/image": "1.93.2",
    "@tloncorp/editor": "workspace:*",
    "@tloncorp/shared": "workspace:*",
    "@uidotdev/usehooks": "^2.4.1",
    "color2k": "^2.0.0",
    "expo-blur": "^12.9.2",
    "expo-haptics": "^12.8.1",
    "expo-linear-gradient": "^13.0.2",
    "lodash": "^4.17.21",
    "moti": "^0.28.1",
    "react-native-context-menu-view": "^1.15.0",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-svg": "^15.0.0",
    "react-tweet": "^3.0.4",
    "tamagui": "1.93.2"
  },
  "peerDependencies": {
    "@10play/tentap-editor": "^0.4.55",
    "@urbit/sigil-js": "^2.2.0",
    "expo-image": "*",
    "react": "*",
    "react-native-safe-area-context": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.93.2"
  }
}
