{
  "name": "homestead",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "14.19.0"
  },
  "scripts": {
    "prepare": "cd .. && husky install ui/.husky",
    "build": "tsc && vite build",
    "build:mock": "tsc && vite build --mode staging",
    "build:profile": "tsc && vite build --mode profile",
    "build:chatmock": "tsc && vite build --mode chatstaging",
    "build:chat": "tsc && vite build --mode chat",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:format": "prettier src/ --write",
    "lint:all": "npm run lint:format && npm run lint:fix",
    "mock": "vite --mode mock",
    "chat": "vite --mode chat",
    "mock:chat": "vite --mode chatmock",
    "serve": "vite preview",
    "test": "vitest",
    "tsc": "tsc --noEmit",
    "postinstall": "patch-package",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "tsc-files --noEmit src/window.ts src/env.d.ts"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run lint:format"
    ]
  },
